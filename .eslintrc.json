{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "rules": {
    "operator-linebreak": ["error", "after"],
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/sort-comp": 0,
    "no-param-reassign": 0,
    "react/prop-types": 0
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "globals": {
    "Generator": true,
    "Iterator": true,
    "Iterable": true,
    "$Keys": true,
    "$Shape": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  },
  "plugins": [
    "react",
    "flowtype",
    "import"
  ],
  "ecmaFeatures": {
    "jsx": true
  }
}